function bmiCalculator(weight, height) {

    var weight = window.prompt("Please enter your weight in kg: ");
    var height = window.prompt("Please enter your height in m: ");
    var bmi =weight/height**2;
    
    return Math.round(bmi);
    
    
}

var bmi= bmiCalculator(50, 1.8);

if (bmi<18.5) {
    console.log("Your BMI is "+ bmi+ " and You are underweight.");
} if (bmi >=18.6 && bmi<=24.9){
    console.log ("Your BMI is "+ bmi+ " and Your weight is mormal.");
} else {
    console.log ("Your BMI is "+ bmi+ " and You are overweight.");
}

